    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.8.0",
    "tailwindcss": "4.1.12",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.8",
      "@types/react-dom": "19.2.3"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
